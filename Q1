#include <iostream>
using namespace std;

class Rectangle {
private:
    int length, breadth;

public:
   
    Rectangle() {
        length = 1;
        breadth = 1;
    }

    
    Rectangle(int l, int b) {
        length = l;
        breadth = b;
    }

  
    int calculateArea() {
        return length * breadth;
    }

   
    void displayArea() {
        cout << "Area of rectangle with length " << length 
             << " and breadth " << breadth 
             << " is: " << calculateArea() << endl;
    }
};

int main() {
  
    Rectangle rect1;
    rect1.displayArea();

   
    Rectangle rect2(5, 3); 
    rect2.displayArea();

    return 0;
}


